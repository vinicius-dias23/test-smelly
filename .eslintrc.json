{
    "env": {
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:jest/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "jest"
    ],
    "rules": {
        "jest/no-disabled-tests": "warn",
        "jest/no-conditional-expect": "error",
        "jest/no-identical-title": "error"
    }
}
